class PhidgetManager extends Phidget {	//private functions	private function userOnConnect(success) {		if (success) {			mySocket.send(Constants.PNET_OPENPM);		}	}	private function gotData(dataArray:Array) {		var temp:Phidget = new Phidget();		temp.setSerialNumber(parseInt(dataArray[5]));		temp.setDeviceVersion(parseInt(dataArray[4]));		temp.setDeviceType(dataArray[2]);		switch(parseInt(dataArray[1])) {			case Constants.PNET_ATTACH:				temp.attached = true;				onAttach(temp);				break;			case Constants.PNET_DETACH:				temp.attached = false;				onDetach(temp);				break;			default:				trace("Got bad Phidget Manager Message");		}	}}